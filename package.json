    "tape": "^5.5.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.3.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": ">=16 <17"
  }
}
